{
  "version": 2,
  "name": "x-2025-recap-oauth",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "functions": {
    "api/auth/login.js": {
      "maxDuration": 10
    },
    "api/auth/callback.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js"
    },
    {
      "src": "/api/auth/callback",
      "dest": "/api/auth/callback.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}